{
  "name": "cross-state",
  "version": "1.0.0",
  "description": "(React) state library",
  "license": "ISC",
  "repository": "schummar/cross-state",
  "author": {
    "name": "Marco Schumacher",
    "email": "marco@schumacher.dev"
  },
  "type": "module",
  "sideEffects": [
    "dist/es/react/register.mjs",
    "dist/es/immer/register.mjs",
    "dist/cjs/react/register.cjs",
    "dist/cjs/immer/register.cjs"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/es/index.mjs"
    },
    "./react": {
      "types": "./dist/types/react/index.d.ts",
      "import": "./dist/es/react/index.mjs",
      "require": "./dist/cjs/react/index.cjs",
      "default": "./dist/es/react/index.mjs"
    },
    "./react/register": {
      "types": "./dist/types/react/register.d.ts",
      "import": "./dist/es/react/register.mjs",
      "require": "./dist/cjs/react/register.cjs",
      "default": "./dist/es/react/register.mjs"
    },
    "./immer": {
      "types": "./dist/types/immer/index.d.ts",
      "import": "./dist/es/immer/index.mjs",
      "require": "./dist/cjs/immer/index.cjs",
      "default": "./dist/es/immer/index.mjs"
    },
    "./immer/register": {
      "types": "./dist/types/immer/register.d.ts",
      "import": "./dist/es/immer/register.mjs",
      "require": "./dist/cjs/immer/register.cjs",
      "default": "./dist/es/immer/register.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/types/index.d.ts"
      ],
      "react": [
        "dist/types/react/index.d.ts"
      ],
      "react/register": [
        "dist/types/react/register.d.ts"
      ],
      "immer": [
        "dist/types/immer/index.d.ts"
      ],
      "immer/register": [
        "dist/types/immer/register.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "runp -i build:clean :p build:compile:*",
    "build:clean": "rimraf dist",
    "build:compile:js": "vite build",
    "build:compile:types": "tsc && tsc-alias",
    "lint": "runp -i lint:*",
    "lint:eslint": "lint",
    "lint:tsc": "tsc --noEmit --emitDeclarationOnly false",
    "lint:tsc:tests": "tsc --noEmit --emitDeclarationOnly false -p test/tsconfig.json",
    "size": "size-limit",
    "prepublishOnly": "runp -i lint test build :p size publint",
    "test": "pnpm test:watch run --coverage",
    "test:watch": "node --expose-gc node_modules/vitest/vitest.mjs",
    "test:ui": "node --expose-gc node_modules/vitest/vitest.mjs --ui",
    "test:export": "pnpm test:watch run --coverage --reporter=verbose --reporter=junit --outputFile=test/testResults.xml"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0",
    "immer": ">=9.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "immer": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@emotion/styled": "11.11.0",
    "@mantine/core": "7.5.0",
    "@mui/material": "5.15.6",
    "@schummar/eslint-config": "github:schummar/eslint-config",
    "@schummar/prettier-config": "github:schummar/prettier-config",
    "@schummar/runp": "2.0.2",
    "@size-limit/preset-small-lib": "11.0.2",
    "@testing-library/react": "14.1.2",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/seedrandom": "3.0.8",
    "@types/use-sync-external-store": "0.0.6",
    "@types/ws": "8.5.10",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.2.1",
    "esbuild": "0.19.12",
    "eslint": "8.56.0",
    "happy-dom": "13.3.1",
    "immer": "10.0.3",
    "jsdom": "24.0.0",
    "prettier": "3.2.4",
    "publint": "0.2.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "seedrandom": "3.0.5",
    "semantic-release": "23.0.0",
    "size-limit": "11.0.2",
    "tsc-alias": "1.8.8",
    "typescript": "5.3.3",
    "use-sync-external-store": "1.2.0",
    "vite": "5.0.12",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.2.1"
  },
  "volta": {
    "node": "18.17.1",
    "pnpm": "8.7.4"
  },
  "eslintConfig": {
    "extends": [
      "@schummar/eslint-config"
    ],
    "rules": {
      "unicorn/expiring-todo-comments": "off"
    }
  },
  "prettier": "@schummar/prettier-config",
  "size-limit": [
    {
      "name": "core",
      "path": "dist/es/index.mjs",
      "limit": "7.5 KB"
    },
    {
      "name": "core: Store",
      "path": "dist/es/index.mjs",
      "import": "{createStore}",
      "limit": "4 KB"
    },
    {
      "name": "core: Cache",
      "path": "dist/es/index.mjs",
      "import": "{createCache}",
      "limit": "5.5 KB"
    },
    {
      "name": "core: InstanceCache",
      "path": "dist/es/index.mjs",
      "import": "{InstanceCache}",
      "limit": "600 B"
    },
    {
      "name": "core: calcDuration",
      "path": "dist/es/index.mjs",
      "import": "{calcDuration}",
      "limit": "100 B"
    },
    {
      "name": "core: urlStore",
      "path": "dist/es/index.mjs",
      "import": "{createUrlStore}",
      "limit": "4 KB"
    },
    {
      "name": "react",
      "path": "dist/es/react/index.mjs",
      "limit": "10 KB"
    },
    {
      "name": "react: useStore",
      "path": "dist/es/react/index.mjs",
      "import": "{useStore}",
      "limit": "3 KB"
    },
    {
      "name": "react: useCache",
      "path": "dist/es/react/index.mjs",
      "import": "{useCache}",
      "limit": "6 KB"
    },
    {
      "name": "/immer",
      "path": "dist/es/immer/index.mjs",
      "limit": "250 B"
    },
    {
      "name": "empty",
      "path": "dist/es/index.mjs",
      "import": "{}",
      "limit": "0"
    }
  ]
}
