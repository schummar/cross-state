{
  "name": "schummar-state",
  "repository": "schummar/schummar-state",
  "version": "0.2.1-experimental-19",
  "description": "(React) state library",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/es/index.js"
    },
    "./react": {
      "import": "./dist/es/react.js",
      "require": "./dist/cjs/react.js",
      "default": "./dist/es/react.js"
    },
    "./immer": {
      "import": "./dist/es/immer.js",
      "require": "./dist/cjs/immer.js",
      "default": "./dist/es/immer.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "react": [
        "dist/types/integrations/react/index.d.ts"
      ],
      "immer": [
        "dist/types/integrations/immer/index.d.ts"
      ]
    }
  },
  "author": {
    "name": "Marco Schumacher",
    "email": "marco@schumacher.dev"
  },
  "license": "ISC",
  "scripts": {
    "start": "tsc --watch",
    "build": "npm-run-all -s build:clean -p build:compile:* -s build:cjsPackage",
    "build:clean": "rimraf \"dist/**/*\"",
    "build:compile:js": "vite build",
    "build:compile:types": "tsc",
    "build:cjsPackage": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --noEmit --emitDeclarationOnly false",
    "prepublishOnly": "npm run lint build",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:export": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=test/testResults.xml"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.14.1",
    "c8": "^7.11.3",
    "esbuild-dev": "^0.10.0",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.26.0",
    "happy-dom": "^5.0.0",
    "immer": "^9.0.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "use-sync-external-store": "^1.1.0",
    "vite": "^2.9.10",
    "vitest": "^0.14.1"
  },
  "peerDependencies": {
    "@types/react": ">=16.0.8",
    "immer": ">=9.0.0",
    "react": ">=16.0.8",
    "use-sync-external-store": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "immer": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "react.js",
    "react.d.ts"
  ]
}
