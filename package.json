{
  "name": "schummar-state",
  "type": "module",
  "repository": "schummar/schummar-state",
  "version": "0.2.1-experimental-21",
  "description": "(React) state library",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js",
      "default": "./dist/es/index.mjs"
    },
    "./react": {
      "import": "./dist/es/integrations/react/index.mjs",
      "require": "./dist/cjs/integrations/react/index.js",
      "default": "./dist/es/integrations/react/index.mjs"
    },
    "./immer": {
      "import": "./dist/es/integrations/immer/index.mjs",
      "require": "./dist/cjs/integrations/immer/index.js",
      "default": "./dist/es/integrations/immer/index.mjs"
    },
    "./persist": {
      "import": "./dist/es/persist/index.mjs",
      "require": "./dist/cjs/persist/index.js",
      "default": "./dist/es/persist/index.mjs"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "react": [
        "dist/types/integrations/react/index.d.ts"
      ],
      "immer": [
        "dist/types/integrations/immer/index.d.ts"
      ]
    }
  },
  "author": {
    "name": "Marco Schumacher",
    "email": "marco@schumacher.dev"
  },
  "license": "ISC",
  "scripts": {
    "start": "tsc --watch",
    "build": "runp -k build:clean :p build:compile:*",
    "build:clean": "rimraf \"dist/**/*\"",
    "build:compile:js": "vite build",
    "build:compile:types": "tsc",
    "lint": "runp lint:*",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --noEmit --emitDeclarationOnly false",
    "prepublishOnly": "npm run lint build",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:export": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=test/testResults.xml"
  },
  "devDependencies": {
    "@schummar/runp": "1.6.3",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/use-sync-external-store": "0.0.3",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@vitejs/plugin-react": "2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "eslint": "8.24.0",
    "eslint-plugin-import": "2.26.0",
    "fast-equals": "4.0.3",
    "happy-dom": "6.0.4",
    "immer": "9.0.15",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "typescript": "4.8.3",
    "use-sync-external-store": "1.2.0",
    "vite": "3.1.3",
    "vitest": "0.23.4"
  },
  "peerDependencies": {
    "@types/react": ">=16.0.8",
    "immer": ">=9.0.0",
    "react": ">=16.0.8",
    "use-sync-external-store": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "immer": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "use-sync-external-store": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "react.js",
    "react.d.ts"
  ],
  "volta": {
    "node": "18.9.0"
  }
}
