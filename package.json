    "ava": "^3.15.0",
    "browser-env": "^3.3.0",
    "eslint": "^7.25.0",
    "immer": "9.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@types/react": ">=16.0.8",
    "immer": "^9.0.1",
